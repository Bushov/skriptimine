#!/bin/bash

## K체sib ridu ja t채rnide arvu reas. 

echo "Sisestage ridade arv: "
read rida
echo "Sisestage t채rnide arv reas: "
read symbol

echo "Ridade arv: $rida ja t채rnide arv: $symbol"

for (( i = 1 ; i <= $rida; i++ ))   ###Outer for loop###

do

	for (( j = 1 ; j <= $symbol; j++)) ###Inner for loop###

	do
		echo -n ""*" "

done

	echo "" #print a new line#

done
