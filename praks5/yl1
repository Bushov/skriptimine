#!/bin/bash

#Kasutaja vanuse k체simine ja seletava kirjelduse v채ljastamine.

echo -ne "Kui vana sa oled: "
read vanus

if test $vanus -gt 0 -a $vanus -lt 11

then
	echo "Oled laps!"
elif test $vanus -ge 11 -a $vanus -lt 18

then
	echo "Oled nooruk!"

elif test $vanus -ge 18 -a $vanus -lt 63

then
	echo "Oled t채iskasvanu!"

elif test $vanus -ge 63

then

	echo "Oled seenior!"

else

	echo "Vigane sisend! Proovige uuesti sisestades t채isarv alates nullist"

fi
